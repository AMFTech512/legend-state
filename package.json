{
    "name": "@legendapp/state",
    "version": "0.9.1-alpha.18",
    "description": "legend-state",
    "sideEffects": false,
    "private": true,
    "main": "./index.js",
    "types": "./index.d.ts",
    "files": [
        "**"
    ],
    "scripts": {
        "clean": "rimraf dist",
        "prebuild": "npm run clean",
        "build": "tsc && cp package.json dist/package.json && rm -rf dist/tests && rm dist/*.tsbuildinfo && json -I -f dist/package.json -e \"this.private=false; this.devDependencies=undefined; this.scripts=undefined\"",
        "test": "jest",
        "preversion": "npm run build",
        "postversion": "git push --follow-tags",
        "jestclear": "jest --clearCache",
        "publish": "npm run build && cd dist && npm publish"
    },
    "exports": {
        ".": "./index.js",
        "./internal": "./internal.js",
        "./local-storage": "./local-storage.js",
        "./mmkv": "./mmkv.js",
        "./react": "./react.js"
    },
    "dependencies": {
        "@legendapp/tools": "^2.0.0-alpha.14"
    },
    "devDependencies": {
        "@testing-library/react-hooks": "^8.0.1",
        "@types/jest": "^26.0.24",
        "@types/react": "^18.0.1",
        "@types/react-dom": "^18.0.0",
        "jest": "^27.0.6",
        "json": "^11.0.0",
        "prettier": "^2.7.1",
        "react": "^18.0.0",
        "react-native-mmkv": "^2.4.3",
        "react-test-renderer": "^18.2.0",
        "rimraf": "^3.0.2",
        "ts-jest": "^27.0.4",
        "tslib": "^2.4.0",
        "typescript": "^4.7.4"
    },
    "peerDependencies": {
        "react": ">=16.8",
        "react-native-mmkv": "^2"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "react-native-mmkv": {
            "optional": true
        }
    },
    "author": "Legend <contact@legendapp.com> (https://github.com/LegendApp)",
    "keywords": [
        "react",
        "react-native",
        "state",
        "hooks",
        "proxy"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/LegendApp/legend-state.git"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/LegendApp/legend-state/issues"
    },
    "homepage": "https://github.com/LegendApp/legend-state#readme",
    "publishConfig": {
        "registry": "https://registry.npmjs.org/"
    }
}
